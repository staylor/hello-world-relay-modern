    "graphql": "^0.9.3",
    "graphql-relay": "^0.5.1",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "kyt": "^0.5.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-relay": "dev",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-plugin-relay": "dev",
    "enzyme": "^2.8.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "relay-compiler": "dev"
  },
  "scripts": {
    "dev": "kyt dev",
    "graphql-dev": "nodemon --exec babel-node src/graphql/server.js",
